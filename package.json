{
  "name": "astro-uploader",
  "version": "1.2.3",
  "description": "A uploader for uploading the Astro generated files through the S3 API.",
  "keywords": [
    "Astro",
    "S3",
    "withastro",
    "opendal"
  ],
  "bugs": {
    "url": "https://github.com/syhily/astro-uploader/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/syhily/astro-uploader"
  },
  "license": "MIT",
  "author": "Yufan Sheng <syhily@gmail.com>",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "lint": "biome check --write . && prettier . --write",
    "stub": "unbuild --stub"
  },
  "dependencies": {
    "mime": "^4.0.6",
    "opendal": "^0.47.7",
    "rimraf": "^6.0.1"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.9.4",
    "@types/node": "^22.10.5",
    "astro": "^5.1.3",
    "prettier": "^3.4.2",
    "prettier-plugin-astro": "^0.14.1",
    "prettier-plugin-astro-organize-imports": "^0.4.11",
    "prettier-plugin-organize-imports": "^4.1.0",
    "unbuild": "^3.2.0"
  }
}
