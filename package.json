{
  "name": "astro-uploader",
  "type": "module",
  "version": "1.3.3",
  "description": "A uploader for uploading the Astro generated files and other static files through the S3 API.",
  "author": "Yufan Sheng <syhily@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/syhily/astro-uploader"
  },
  "bugs": {
    "url": "https://github.com/syhily/astro-uploader/issues"
  },
  "keywords": [
    "astro",
    "astro-component",
    "opendal",
    "s3",
    "withastro"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup-node",
    "lint": "eslint",
    "lint:fix": "eslint --fix"
  },
  "peerDependencies": {
    "astro": "^5"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.850.0",
    "mime": "^4.0.7"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^4.18.0",
    "@types/node": "^24.1.0",
    "astro": "^5.12.2",
    "eslint": "^9.31.0",
    "tsup": "^8.5.0",
    "typescript": "^5.8.3"
  }
}
