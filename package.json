{
  "name": "astro-uploader",
  "version": "1.1.0",
  "description": "A uploader for uploading the Astro generated files through the S3 API.",
  "keywords": ["Astro", "S3", "withastro", "opendal"],
  "bugs": {
    "url": "https://github.com/syhily/astro-uploader/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/syhily/astro-uploader"
  },
  "license": "MIT",
  "author": "Yufan Sheng <syhily@gmail.com>",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "default": "./dist/index.mjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": ["dist"],
  "scripts": {
    "build": "unbuild",
    "format": "biome format . --write",
    "stub": "unbuild --stub"
  },
  "dependencies": {
    "mime": "^4.0.4",
    "opendal": "^0.47.0",
    "rimraf": "^5.0.8"
  },
  "devDependencies": {
    "@biomejs/biome": "^1.8.3",
    "@types/node": "^20.14.10",
    "astro": "^4.11.5",
    "unbuild": "^2.0.0"
  }
}
